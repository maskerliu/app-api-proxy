{
  "name": "app-api-proxy",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/electron/main.cjs",
  "license": "UNLICENSED",
  "scripts": {
    "test": "node --import tsx .build-scripts/test.ts",
    "build": "node --experimental-json-modules --max_old_space_size=3500 --import tsx .build-scripts/builder.ts",
    "build:clean": "cross-env BUILD_TARGET=clean node --experimental-json-modules --import tsx .build-scripts/builder.ts",
    "build:web": "cross-env BUILD_TARGET=web node --experimental-json-modules --import tsx .build-scripts/builder.ts",
    "dev": "node --experimental-json-modules --import tsx .build-scripts/runner.dev.ts",
    "pub": "yarn build && electron-builder",
    "asar": "node .build-scripts/compress-asar.js"
  },
  "build": {
    "productName": "app-api-proxy",
    "appId": "com.lynxchina.desktop.apiProxy",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "!**/node_modules/**/*.map",
      "!**/node_modules/.github/*",
      "!**/node_modules/**/*.md",
      "!**/node_modules/**/LICENSE",
      "!**/node_modules/**/android-*/*",
      "!**/node_modules/**/linux-*/*",
      "!**/node_modules/**/win32-ia32/*",
      "!**/node_modules/**/deps/*"
    ],
    "extraResources": [],
    "extraFiles": [],
    "pkg": {
      "installLocation": "/Applications"
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        }
      ],
      "electronLanguages": [
        "zh_CN",
        "en"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "electronLanguages": [
        "zh_CN",
        "en"
      ]
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    },
    "linux": {
      "icon": "icons",
      "target": "snap"
    }
  },
  "resolutions": {
    "leveldown": "^6.1.1",
    "node-gyp": "^10.2.0",
    "glob": "^11.0.0",
    "@electron/get": "^3.1.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "inversify": "^6.0.3",
    "json-bigint": "^1.0.0",
    "pouchdb-adapter-leveldb": "^9.0.0",
    "pouchdb-find": "^9.0.0",
    "pouchdb-node": "^9.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.1.0",
    "sockjs": "^0.3.24",
    "systeminformation": "^5.12.11"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-serve-static-core": "^5.0.0",
    "@types/json-bigint": "^1.0.4",
    "@types/node": "22.7.7",
    "@types/pouchdb": "^6.4.2",
    "@types/pouchdb-find": "^7.3.3",
    "@types/progress-stream": "^2.0.5",
    "@types/sockjs": "0.3.36",
    "@types/sockjs-client": "^1.5.4",
    "@types/source-map-support": "^0.5.10",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/webpack-hot-middleware": "2.25.9",
    "@vant/touch-emulator": "^1.4.0",
    "@vue/compiler-sfc": "^3.5.12",
    "ace-builds": "^1.36.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.7",
    "brace": "^0.11.1",
    "chalk": "^5.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "del": "^8.0.0",
    "electron": "33.0.1",
    "electron-builder": "^25.1.8",
    "electron-builder-squirrel-windows": "^25.1.8",
    "electron-debug": "^4.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.2",
    "md5": "^2.3.0",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "pinia": "^2.2.4",
    "qrcode.vue": "^3.5.1",
    "rimraf": "^6.0.1",
    "sockjs-client": "^1.6.1",
    "source-map-loader": "^5.0.0",
    "source-map-support": "^0.5.21",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "url-loader": "^4.1.1",
    "vant": "^4.9.8",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-json-pretty": "^2.4.0",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.4.5",
    "vue-style-loader": "^4.1.3",
    "vue3-infinite-list": "^0.2.5",
    "webpack": "^5.95.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-hot-middleware": "^2.26.1"
  },
  "packageManager": "yarn@4.5.1+sha512.341db9396b6e289fecc30cd7ab3af65060e05ebff4b3b47547b278b9e67b08f485ecd8c79006b405446262142c7a38154445ef7f17c1d5d1de7d90bf9ce7054d"
}
