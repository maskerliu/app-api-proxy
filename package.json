{
  "name": "app-api-proxy",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "./dist/electron/main.js",
  "license": "UNLICENSED",
  "scripts": {
    "build": "cross-env node --loader ts-node/esm --experimental-json-modules --max_old_space_size=9000 .build-scripts/builder.ts",
    "build:clean": "cross-env BUILD_TARGET=clean node --loader ts-node/esm --experimental-json-modules .build-scripts/builder.ts",
    "build:web": "cross-env BUILD_TARGET=web node --loader ts-node/esm --experimental-json-modules .build-scripts/builder.ts",
    "dev": "node --loader ts-node/esm --experimental-json-modules .build-scripts/runner.dev.ts",
    "pub": "yarn build && electron-builder",
    "asar": "node .build-scripts/compress-asar.js"
  },
  "build": {
    "productName": "app-api-proxy",
    "appId": "com.lynxchina.desktop.apiProxy",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/*/**/*"
    ],
    "extraResources": [],
    "pkg": {
      "installLocation": "/Applications"
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": [
        "pkg"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "aedes": "^0.48.0",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "json-bigint": "^1.0.0",
    "pouchdb": "^7.3.0",
    "pouchdb-find": "^7.3.0",
    "progress-stream": "^2.0.0",
    "protobufjs": "^7.1.0",
    "sockjs": "^0.3.24"
  },
  "devDependencies": {
    "mqtt": "^4.3.7",
    "@types/body-parser": "1.19.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/cors": "^2.8.12",
    "@types/node": "18.8.2",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-find": "^7.3.0",
    "@types/progress-stream": "^2.0.2",
    "@types/sockjs": "0.3.33",
    "@types/sockjs-client": "^1.5.1",
    "@types/webpack-bundle-analyzer": "^4.4.3",
    "@types/webpack-dev-middleware": "5.0.2",
    "@types/webpack-hot-middleware": "2.25.6",
    "@vant/touch-emulator": "^1.4.0",
    "@vue/compiler-sfc": "^3.2.40",
    "adm-zip": "^0.5.9",
    "axios": "^1.0.0",
    "chalk": "^5.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "del": "^7.0.0",
    "electron": "21.0.1",
    "electron-builder": "^23.3.3",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "md5": "^2.3.0",
    "mini-css-extract-plugin": "2.5.3",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "qrcode.vue": "^3.3.3",
    "sass-loader": "^12.6.0",
    "sockjs-client": "^1.6.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "throttle-typescript": "^1.1.0",
    "url-loader": "^4.1.1",
    "vant": "^3.6.3",
    "vue": "^3.2.38",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.1.5",
    "pinia": "^2.0.22",
    "vue-style-loader": "^4.1.3",
    "vue3-infinite-list": "^0.2.5",
    "json-editor-vue3": "^1.0.6",
    "webpack": "^5.74.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.25.2"
  }
}