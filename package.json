{
  "name": "app-api-proxy",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/electron/main.cjs",
  "license": "UNLICENSED",
  "scripts": {
    "test": "node --import tsx .build-scripts/test.ts",
    "build": "node --experimental-json-modules --max_old_space_size=3500 --import tsx .build-scripts/builder.ts",
    "build:clean": "cross-env BUILD_TARGET=clean node --experimental-json-modules --import tsx .build-scripts/builder.ts",
    "build:web": "cross-env BUILD_TARGET=web node --experimental-json-modules --import tsx .build-scripts/builder.ts",
    "dev": "node --experimental-worker --experimental-json-modules --import tsx .build-scripts/runner.dev.ts",
    "pub": "yarn build && electron-builder",
    "asar": "node .build-scripts/compress-asar.js"
  },
  "build": {
    "productName": "app-api-proxy",
    "appId": "com.lynxchina.desktop.apiProxy",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/*/**/*",
      "!**/node_modules/**/*.map",
      "!**/node_modules/.github/*",
      "!**/node_modules/**/*.md",
      "!**/node_modules/**/LICENSE",
      "!**/node_modules/**/android-*/*",
      "!**/node_modules/**/linux-*/*",
      "!**/node_modules/**/win32-*/*"
    ],
    "extraResources": [],
    "pkg": {
      "installLocation": "/Applications"
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": [
        "pkg"
      ],
      "electronLanguages": [
        "zh_CN",
        "en"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons",
      "target": "snap"
    }
  },
  "dependencies": {
    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "lynx-express-mvc": "1.0.14",
    "pouchdb-find": "^7.3.0",
    "pouchdb-node": "^7.3.0",
    "sockjs": "^0.3.24",
    "systeminformation": "^5.12.11"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/express-fileupload": "^1.4.1",
    "@types/node": "22.5.4",
    "@types/pouchdb": "^6.4.2",
    "@types/pouchdb-find": "^7.3.3",
    "@types/progress-stream": "^2.0.5",
    "@types/sockjs": "0.3.36",
    "@types/sockjs-client": "^1.5.4",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/webpack-dev-middleware": "5.3.0",
    "@types/webpack-hot-middleware": "2.25.9",
    "@vant/touch-emulator": "^1.4.0",
    "@vue/compiler-sfc": "^3.5.4",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "del": "^7.1.0",
    "echarts": "^5.5.1",
    "electron": "32.0.0",
    "electron-builder": "^25.0.5",
    "electron-debug": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "md5": "^2.3.0",
    "mini-css-extract-plugin": "2.9.1",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "pinia": "^2.2.2",
    "qrcode.vue": "^3.4.1",
    "sass-loader": "^16.0.1",
    "sockjs-client": "^1.6.1",
    "style-loader": "^4.0.0",
    "throttle-typescript": "^1.1.0",
    "ts-loader": "^9.5.1",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "url-loader": "^4.1.1",
    "vant": "^3.6.3",
    "vue": "^3.4.38",
    "vue-i18n": "^10.0.1",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.4.3",
    "vue-style-loader": "^4.1.3",
    "vue3-infinite-list": "^0.2.5",
    "vue3-ace-editor": "^2.2.4",
    "ace-editor-vue3": "^1.1.2",
    "source-map-loader": "^5.0.0",
    "webpack": "^5.94.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-hot-middleware": "^2.26.1"
  }
}